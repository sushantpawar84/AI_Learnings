# Copy this file to .env and fill in your actual credentials
# The .env file will be loaded automatically by python-dotenv

# SVN Credentials
SVN_URL=https://svn.example.com/project/trunk
SVN_USERNAME=your_svn_username
SVN_PASSWORD=your_svn_password

# Git Credentials
GIT_URL=https://github.com/myorg/myproject.git
GITHUB_TOKEN=your_github_personal_access_token

# Migration Settings
BATCH_SIZE=50
BATCH_TIMEOUT_SECONDS=300
RETRY_ATTEMPTS=3

# Logging
LOG_LEVEL=INFO

# Database
DB_PATH=./migration_state.db
LOG_DIR=./logs

